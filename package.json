{
  "name": "typeland",
  "author": "@narze",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:base-css": "tailwindcss build ./styles/tailwind.base.css -o ./styles/base.css",
    "type-check": "tsc --pretty --noEmit && tsc --pretty --noEmit -p cypress",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test --ci --coverage && yarn cy:ci",
    "cy:run": "cypress run --browser chrome",
    "cy:run:headless": "cypress run --browser chrome --headless",
    "cy:open": "cypress open",
    "e2e": "start-test dev 3000 cy:open",
    "cy:ci": "start-test start 3000 cy:run:headless",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "abortcontroller-polyfill": "^1.5.0",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "firebase": "^7.22.1",
    "next": "9.5.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.2",
    "use-reducer-async": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@emotion/babel-plugin": "^11.0.0-next.17",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@tailwindcss/ui": "^0.6.2",
    "@tailwindcssinjs/macro": "^0.10.0",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "2.8.0",
    "cypress": "5.3.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-react": "^7.21.3",
    "firebase-mock": "^2.3.2",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "jest-emotion": "^10.0.32",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "start-server-and-test": "^1.11.5",
    "tailwindcss": "^1.8.12",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
